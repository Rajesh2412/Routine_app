
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // This rule allows anyone to read and write to your database.
    // It is useful for development but is insecure for production.
    // You should replace this with more secure rules before launch.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
